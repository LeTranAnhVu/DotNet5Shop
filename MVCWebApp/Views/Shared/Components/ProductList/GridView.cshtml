@using MVCWebApp.ViewComponents
@model List<DataAccess.Models.Product>
<div class="product-list">
    <div class="row">
        @foreach (var product in Model)
        {
            <div class="col-md-4 mb-5">
                <vc:product product="@product" variant="@ProductViewVariant.SimpleCard"/>
            </div>
        }
    </div>
</div>